{
  "name": "Group Tabs",
  "description": "Group tabs by domain.",
  "version": "1.1.0",
  "manifest_version": 3,
  "background": {},
  "permissions": ["tabs", "tabGroups"],
  "action": {
    "default_popup": "menu.html",
    "default_icon": {
      "16": "/images/gt_icon16.png",
      "32": "/images/gt_icon32.png",
      "48": "/images/gt_icon48.png",
      "128": "/images/gt_icon128.png"
    }
  },
  "icons": {
    "16": "/images/gt_icon16.png",
    "32": "/images/gt_icon32.png",
    "48": "/images/gt_icon48.png",
    "128": "/images/gt_icon128.png"
  }
}
